{% extends "@PSProject\\layout.html.twig" %}

{% block title %}
	Contact
{% endblock %}

{% block content %}
<container class="container-project container-fluid">
	<div class="project-article col-lg-10 offset-lg-1">
		<h2><span class="lettrine">C</span>ontact us</h2>
        <form action="" method="post" class="form container">
            <form>
                <div class="form-group">
                    <label for="username" class="col-sm-2 control-label">Your email</label>
                    <input class="form-control" type="text" id="username" name="_username" required="required" />
                </div>
                <div class="form-group">
                    <label for="object" class="col-sm-2 control-label">Your object</label>
					<input class="form-control" type="text" id="object" name="_object" required="required" />
                </div>
                <div class="form-group">
                    <label for="message" class="col-sm-2 control-label">Your message</label>
                    <textarea class="form-control" rows="5" id="message"  name="_message" required="required"></textarea>
                </div>
                <div class="form-group login">     
                    <input type="submit" class="btn cancel-btn" id="_submit" name="_submit"/>
                </div>
           </form>
        </form>
	</div>
</container>
{% endblock %}

